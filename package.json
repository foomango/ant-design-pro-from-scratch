  "scripts": {
    "build": "yarn clean-dist && webpack -p --config=config/webpack/prod.js",
    "ci": "yarn lint",
    "clean-dist": "rimraf dist/*",
    "lint": "yarn lint:script",
    "lint-fix": "yarn lint-fix:script",
    "lint-fix:script": "yarn lint:script --fix",
    "lint:script": "eslint . --ext '.js,.jsx,.ts,.tsx'",
    "start": "yarn start-dev",
    "start-dev": "webpack-dev-server --config=config/webpack/dev.js --port=8085",
    "start-prod": "yarn build && node express.js",
    "test": "echo todo"
